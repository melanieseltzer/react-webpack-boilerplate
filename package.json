    "type": "git",
    "url": "git+ssh://git@github.com/melanieseltzer/react-webpack-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "pug",
    "ejs",
    "handlebars",
    "hmr",
    "boilerplate"
  ],
  "author": "Melanie Seltzer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/melanieseltzer/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/melanieseltzer/react-webpack-boilerplate#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "brotli-webpack-plugin": "^0.5.0",
    "compression-webpack-plugin": "^1.1.8",
    "css-loader": "^0.28.10",
    "ejs-loader": "^0.3.0",
